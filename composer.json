{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "nikic/fast-route":  "^1.3",
        "ext-redis": "*",
        "ext-json": "*",
        "symfony/http-foundation" : "^4.3",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
